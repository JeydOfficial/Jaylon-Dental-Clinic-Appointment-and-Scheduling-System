{
  "builds": [
    {
      "src": "main_system/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main_system/wsgi.py"
    }
  ],
  "functions": {
    "main_system/wsgi.py": {
      "maxDuration": 30
    }
  }
}